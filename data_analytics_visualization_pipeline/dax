VAR TTransferVolumeSum = CALCULATE(SELECTEDVALUE(Bitcoin_Onchain_Analytics_Stats[Transfer Volume]), Bitcoin_Onchain_Analytics_Stats[Date] = LatestDate)
RETURN TTransferVolumeSum

